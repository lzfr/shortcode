{
    "name": "lzfr/shortcode",
    "description": "generate short code",
    "autoload": {
        "psr-4": {
            "Lzfr\\Shortcode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lzfr\\Shortcode\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel" : {
            "providers": {
                "Lzfr\\Shortcode\\ShortcodeServiceProvider": "src/Providers/ShortcodeServiceProvider.php"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "lz",
            "email": "lzbrest@yahoo.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },

    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
